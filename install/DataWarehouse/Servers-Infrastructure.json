{
    "unit_structures":[{
        "title": "Servers Infrastructure",
        "description": "Diagnostic and health data of all Servers in infrastructure",
        "data_structures": [{
            "fields": {
                "cpu_usage": "double",
                "mem_usage": "bigint",
                "swap_usage": "bigint",
                "disk1_usage": "bigint",
                "disk2_usage": "bigint",
                "uptime_days": "bigint"
            },
            "data_table_suffix": "server_hw_status",
            "retention_policy": "90 days"
        },{
            "fields": {
                "total_containers": "integer",
                "running_containers": "integer",
                "fail_containers": "integer",
                "docker_status": "smallint"
            },
            "data_table_suffix": "server_docker_status",
            "retention_policy": "90 days"
        }],
        "alerts":{
            "cpu_usage": {
                "rules": [
                    {
                      "measure": "real",
                      "alert_if": "greater",
                      "alert_value": 95
                    }
                  ],
                  "data_table_suffix": "server_hw_status",
                  "title": "CPU usage",
                  "property": "cpu",
                  "data_field": "cpu_usage"
            },
            "mem_usage": {
                "rules": [
                    {
                      "measure": "percent",
                      "alert_if": "greater",
                      "alert_value": 85
                    }
                  ],
                  "data_table_suffix": "server_hw_status",
                  "title": "Memory usage",
                  "property": "memory",
                  "data_field": "mem_usage"
            
            }, 
            "disk1_usage": {
                "rules": [
                    {
                      "measure": "percent",
                      "alert_if": "greater",
                      "alert_value": 70
                    }
                  ],
                  "data_table_suffix": "server_hw_status",
                  "title": "Disk1 usage",
                  "property": "disk1",
                  "data_field": "disk1_usage"
            },
            "disk2_usage": {
                "rules": [
                    {
                      "measure": "percent",
                      "alert_if": "greater",
                      "alert_value": 70
                    }
                  ],
                  "data_table_suffix": "server_hw_status",
                  "title": "Disk2 usage",
                  "property": "disk2",
                  "data_field": "disk2_usage"
            },
            "uptime_days": {
                "rules": [
                    {
                      "measure": "real",
                      "alert_if": "less_or_equal",
                      "alert_value": 1
                    }
                ],
                "data_table_suffix": "server_hw_status",
                "title": "Uptime",
                "property": "uptime",
                "data_field": "uptime_days"
            }
        }
    }],
    "unit_groups":[{
        "title": "Bare Metals"
    },{
        "title": "Virtual Machines"
    },{
        "title": "Docker Machines",
        "alerts": {
            "fail_containers": {
                "rules": [
                    {
                      "measure": "real",
                      "alert_if": "greater_or_equal",
                      "alert_value": 1
                    }
                ],
                "data_table_suffix": "server_docker_status",
                "title": "Fail containers",
                "property": "containers",
                "data_field": "fail_containers"
            },
            "docker_status": {
                "rules": [
                    {
                      "measure": "real",
                      "alert_if": "equal",
                      "alert_value": 0
                    }
                ],
                "data_table_suffix": "server_docker_status",
                "title": "Docker status",
                "property": "docker",
                "data_field": "docker_status"
            }
        }
    },{
        "title": "Kubernetes Clusters"
    },{
        "title": "Kubernetes Nodes"
    }],
